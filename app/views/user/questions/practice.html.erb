<%= render 'user/sessions/header' %>
<h2>練習問題</h2>
<p><%= @question.title %></p>

<% @question.question_contents.each_with_index do |content, index|%>
<h4>問題<%=index+1%></h4>
<p><%= content.content %></p>

<h4>答え<%=index+1%></h4>
<p><%= content.answer %></p>
<%end%>

<%= link_to"テストを受ける",user_questions_test_path %>